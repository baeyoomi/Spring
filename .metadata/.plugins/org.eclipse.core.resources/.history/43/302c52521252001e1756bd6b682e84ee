<script>
	class box1{
		abc(){
			this.msg = "홍길동님 환영합니다.";
			return this.msg;
		}
	}
	class box2 extends box1{
		bbb(){
			this.data = this.abc();
			console.log(this.data);
		}
		
	}
	
	let aa = new box2();
	aa.bbb();
	
</script>
</html>